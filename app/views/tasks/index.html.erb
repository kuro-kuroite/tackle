<div class="container">
  <div class="row">
    <div class="col-md-6 offset-md-3">
      <h1>All Tasks</h1>
      <% @tasks.each do |task| %>
        <div class="container">
          <div class="row">
            <%= render task %>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>
